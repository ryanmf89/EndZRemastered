<configs>

<!--MAKES OLD CASH THE MAIN MONEY-->
<set xpath="/traders/@currency_item">oldCash</set>

<!--TRADER ALWAYS OPEN-->
<set xpath="//trader_info/@open_time">04:00</set>
<set xpath="//trader_info/@close_time">04:00</set>
	
<!--TRADER REFRESH STOCK-->
<set xpath="/traders/trader_info[@id='1']/@reset_interval">2</set>
<set xpath="/traders/trader_info[@id='6']/@reset_interval">2</set>
<set xpath="/traders/trader_info[@id='2']/@reset_interval">2</set>
<set xpath="/traders/trader_info[@id='7']/@reset_interval">2</set>
<set xpath="/traders/trader_info[@id='8']/@reset_interval">2</set>

</configs>